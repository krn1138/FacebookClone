<div class="editing">
  <p id="notice"><%= notice %></p>
  <div class="alert alert-success"><h1>確認する</h1></div>
  <p>
    <strong>Image:</strong>
    <%= image_tag @picture.image.url %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= @picture.content %>
  </p>

  <%= link_to 'Edit', edit_picture_path(@picture) %> |
  <%= link_to 'Back', pictures_path %>
</div>
